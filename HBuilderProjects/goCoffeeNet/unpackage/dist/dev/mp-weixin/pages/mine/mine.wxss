.list {
  width: 96%;
  padding-top: 30px;
  padding-left: 4%;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.list .row {
    widows: 100%;
    min-height: 90rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
