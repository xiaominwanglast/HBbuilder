}
.list .row .title {
      font-size: 32rpx;
      color: #333;
}
.list .row .right {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
